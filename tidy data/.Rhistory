ggplot(ngrams.main.2, aes(speaker, LL)) +
geom_point(color="white") +
geom_text(aes(label=ngrams.main.2$word, size=log(ngrams.main.2$word.total),
color=ngrams.main.2$speaker), alpha=0.9) +
scale_size(range=c(5,10)) +
